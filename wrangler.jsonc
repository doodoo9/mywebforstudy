{
    "name": "mywebforstudy",
    "compatibility_date": "2026-01-27",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "main": "worker.js",
    "assets": {
        "directory": "./dist"
    },
    // Run `npx wrangler kv:namespace create USERS` and paste the ID below
    "kv_namespaces": [
        {
            "binding": "USERS",
            "id": "YOUR_KV_ID_HERE"
        }
    ]
}